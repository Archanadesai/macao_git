        li.profile-nav.onhover-dropdown.pe-0.py-0
          .d-flex.align-items-center.profile-media
            img.b-r-10(src='../assets/images/dashboard/profile.png', alt='')
            .flex-grow-1
              span Emay Walter
              //- p.mb-0 Admin 
              //-   i.middle.fa-solid.fa-angle-down
          ul.profile-dropdown.onhover-show-div
            li
              a(href='sign-up.html')
                i(data-feather='user')
                span Account 
            li
              a(href='mail-box.html')
                i(data-feather='mail')
