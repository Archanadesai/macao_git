- var page_Title = 'Default Dashboard';
- var theme_customizer      = true;
- var apex                  = true;
- var sidebar               = true;
- var scrollbar             = true;
- var notify                = true;
- var counter               = true;
- var animate               = true;
- var typeahead             = true;
- var typeahead_search      = true;
- var select_dt             = true;
- var datatable             = true;
- var page_datatable        = true;
- var slick                 = true;
- var header_slider         = true;
- var clock                 = true;
- var dashboard_default     = true;

doctype html
html(lang='en')
  include ../../components/header-files
  body(onload='startTime()')
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid         
            .page-title
              .row
                .col-sm-6
                  h3
                    | Default 
                .col-sm-6
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Dashboard
                    li.breadcrumb-item.active Default      
          // Container-fluid starts
          .container-fluid.default-dashboard
            .row.widget-grid
              .col-xl-3   
                .card.bg-light-primary
                  .card-body   
                    .balance-wrapper
                      .balance-detail 
                        p New Customer
                        span 68
                      .customer-card.bg-light-primary 
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    .balance1 
                      p 10%
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#up-arrow')
                      span | This month
              .col-xl-3   
                .card.bg-light-secondary
                  .card-body   
                    .balance-wrapper
                      .balance-detail 
                        p New Customer
                        span 68
                      .customer-card.bg-light-secondary 
                        svg.stroke-icon.stroke-secondary
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    .balance1 
                      p.txt-secondary 10%
                        svg.stroke-icon.stroke-secondary
                          use(href='../assets/svg/icon-sprite.svg#up-arrow')
                      span | This month
              .col-xl-3   
                .card.bg-light-success
                  .card-body   
                    .balance-wrapper
                      .balance-detail 
                        p New Customer
                        span 68
                      .customer-card.bg-light-success 
                        svg.stroke-icon.stroke-success
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    .balance1 
                      p.txt-success 10%
                        svg.stroke-icon.stroke-success
                          use(href='../assets/svg/icon-sprite.svg#up-arrow')
                      span | This month
              .col-xl-3   
                .card.bg-light-warning
                  .card-body   
                    .balance-wrapper
                      .balance-detail 
                        p New Customer
                        span 68
                      .customer-card.bg-light-warning 
                        svg.stroke-icon.stroke-warning
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    .balance1 
                      p.txt-warning 10%
                        svg.stroke-icon.stroke-warning
                          use(href='../assets/svg/icon-sprite.svg#up-arrow')
                      span | This month                                                                    
          // Container-fluid Ends
        include ../../components/footer        
    include ../../components/footer-files